{
  "repositories": [
    {
      "type": "composer",
      "url": "https://wpackagist.org"
    },
    {
      "type": "package",
      "package": {
        "name": "wordpress/wordpress",
        "type": "webroot",
        "version": "4.9.2",
        "dist": {
          "type": "zip",
          "url": "https://github.com/WordPress/WordPress/archive/4.9.2.zip"
        },
        "require": {
          "fancyguy/webroot-installer": "1.1.0"
        }
      }
    }
  ],
  "require": {
    "wordpress/wordpress": "4.9.2",
    "fancyguy/webroot-installer": "1.1.0",
    "wpackagist-plugin/autodescription": "3.*",
    "wpackagist-plugin/configure-smtp": "3.*",
    "wpackagist-plugin/google-analytics-for-wordpress": "6.*",
    "wpackagist-plugin/imsanity": "2.*",
    "wpackagist-plugin/wordfence": "7.*"
  },
  "extra": {
    "installer-paths": {
      "app/themes/{$name}": [
        "type:wordpress-theme"
      ],
      "app/plugins/{$name}": [
        "type:wordpress-plugin"
      ]
    },
    "webroot-dir": "wp",
    "webroot-package": "wordpress/wordpress"
  }
}